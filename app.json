{
  "expo": {
    "name": "BroadCast",
    "slug": "broadcast",
    "scheme": "broadcast",
    "version": "2.1.21",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "API_URL": "process.env.API_URL"
    },
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpg",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ant_apps.BroadCast",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/icon.jpg"
    },
    "plugins": [
      "expo-web-browser",
      "expo-secure-store",
      "expo-video",
      "expo-build-properties",
      "@stream-io/video-react-native-sdk",
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "App requires camera access",
          "microphonePermission": "App requires microphone access"
        }
      ]
    ]
  }
}